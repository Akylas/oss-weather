{
    "scripts": {
        "prepare.android.benchmark": "cross-var ns prepare android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc  --env.includeDefaultLocation",
        "build.android.baselineprofile": "npm run prepare.android.benchmark;cd platforms/android;./gradlew :benchmark:connectedBenchmarkAndroidTest -Pandroid.testoptions.manageddevices.emulator.gpu=\"swiftshader_indirect\" -Pandroid.testInstrumentationRunnerArguments.androidx.benchmark.enabledRules=BaselineProfile -Dorg.gradle.workers.max=4 --stacktrace; mv -f benchmark/build/outputs/managed_device_android_test_additional_output/pixel2Api31/BaselineProfileGenerator_startup-baseline-prof.txt ../../App_Resources/Android/src/main/baseline-prof.txt",
        "sentry-set-commits": "sentry-cli releases set-commits --auto --ignore-missing",
        "run.android.production": "cross-var ns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sentry=0 --env.sourceMap --env.noconsole=0  --config=nativescript.config.logging.js --env.includeDefaultLocation --env.includeOWMKey",
        "run.android.production.clean": "cross-var ns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sentry=0 --env.sourceMap=0 --env.includeDefaultLocation --env.includeOWMKey",
        "run.android.production.sentry": "cross-var ns run android --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc_sentry --env.sentryUpload=0 --env.sourceMap=0 --config=nativescript.config.sentrylogging.js  --env.devlog",
        "run.android.timeline": "cross-var ns run android --config=nativescript.config.timeline --release --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.timeline --env.includeDefaultLocation --env.includeOWMKey",
        "build.android.production": "cross-var ns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.sentry=0 --env.sourceMap=0  --env.report --aab",
        "build.android.production.fdroid": "cross-var ns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.report --gradleArgs=-PsplitEnabled  --gradleArgs=-PabiFilters=arm64-v8a,armeabi-v7a",
        "build.android.production.fdroid.unsigned": "cross-var ns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.report --gradleArgs=-PsplitEnabled  --gradleArgs=-PabiFilters=arm64-v8a,armeabi-v7a --gradleArgs=-PunsignedApk",
        "build.android.production.fdroid.sentry": "npm run build.android.production.fdroid -- --config=nativescript.config.sentrylogging.js --env.adhoc_sentry",
        "weathermap": "webpack --config weathermap.webpack.config.js --env production",
        "run.ios.device": "ns run ios --no-hmr",
        "run.ios.device.production": "ns run ios --bundle --release --env.production --env.adhoc"
    },
